<ScrollPane fitToHeight="true" fitToWidth="true" focusTraversable="false" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.component.main.MainMenu.Header.HeaderComponentController">
